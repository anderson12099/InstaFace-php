{% extends 'base.html.twig' %}

{% block title %}Imagen{% endblock %}

{% block body %}
    <h1>Imagen</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ imagen.id }}</td>
            </tr>
            <tr>
                <th>Nombre</th>
                <td>{{ imagen.nombre }}</td>
            </tr>
            <tr>
                <th>Descripcion</th>
                <td>{{ imagen.descripcion }}</td>
            </tr>
            <tr>
                <th>Foto</th>
                <td>{{ imagen.foto }}</td>
            </tr>
            <tr>
                <th>Created_at</th>
                <td>{{ imagen.createdAt ? imagen.createdAt|date('Y-m-d') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('imagen_index') }}">back to list</a>

    <a href="{{ path('imagen_edit', {'id': imagen.id}) }}">edit</a>

    {{ include('imagen/_delete_form.html.twig') }}
{% endblock %}
